<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="acc.dao">

<select id="selectAll" resultType="map">
  SELECT USERNO
		,USERID
		,PWD
		,USERNAME
		,PHONENUM
	FROM M_MEMBER
	ORDER BY USERNO ASC
</select>

</mapper>